    "boom": "~7.2.0",
    "bunyan": "~1.8.3",
    "common-tags": "~1.7.2",
    "express": "~4.16.3",
    "express-bunyan-logger": "~1.3.1",
    "express-jwt": "~5.3.1",
    "highlight.js": "~9.12.0",
    "joi": "~13.2.0",
    "json-size": "~1.0.0",
